server.port=9999

#드라이버클래스이름이 oracle.jdbc.driver.OracleDriver에서 oracle.jdbc.OracleDriver로 변경됨
spring.datasource.hikari.driver-class-name=net.sf.log4jdbc.sql.jdbcapi.DriverSpy
spring.datasource.hikari.jdbc-url=jdbc:log4jdbc:oracle:thin:@localhost:49161:xe
spring.datasource.hikari.username=test
spring.datasource.hikari.password=test
spring.datasource.hikari.minimum-idle=2

#JPA 
spring.jpa.database=oracle
spring.jpa.hibernate.ddl-auto=update
spring.jpa.generate-ddl=true
spring.jpa.show-sql=true
spring.jpa.database-platform=org.hibernate.dialect.Oracle10gDialect
#SQL자동줄바꿈을 넣어줌
spring.jpa.properties.hibernate.format_sql=true
