package gui;

import javax.swing.JButton;
import javax.swing.JFrame;

public class Race {
	private JFrame jf;
	private JButton btStart, btReady;
	public Race() {
		jf = new JFrame("달리기");
		btStart = new JButton("달려");
		btReady = new JButton("준비");
		
		Container c = jf.getContentPane();
		jf.setSize(500, 300);
		jf.setVisible(true);
	}
	public static void main(String[] args) {
		new Race();

	}
}
